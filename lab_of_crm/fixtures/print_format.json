  "html": "<style>\n\n  @page {\n\n    margin: 0;\n\n    size: A4;\n\n  }\n \n  body, html {\n\n    margin: 0 !important;\n\n    padding: 0 !important;\n\n    font-family: \"Helvetica\", sans-serif;\n\n  }\n \n  .print-format {\n\n    margin: 0 !important;\n\n    padding: 0 !important;\n\n  }\n \n  .page {\n\n    position: relative;\n\n    box-sizing: border-box;\n\n    min-height: 277mm;\n\n    padding: 15mm 20mm 25mm 20mm;\n\n  }\n \n  .page:not(:first-of-type) {\n\n    padding: 5mm 20mm 15mm 20mm;\n\n  }\n \n  .info-table-wrapper {\n\n    display: flex;\n\n    gap: 20px;\n\n    margin-top: 10px;\n\n    font-size: 10pt;\n\n  }\n \n  .info-table-wrapper table {\n\n    border-collapse: collapse;\n\n    width: 50%;\n\n  }\n \n  .info-table-wrapper td {\n\n    border: 1.5px solid #3c3835;\n\n    padding: 6px;\n\n    vertical-align: top;\n\n  }\n \n  .info-table-wrapper td:first-child {\n\n    width: 40%;\n\n  }\n \n  .quotation-table {\n\n    width: 100%;\n\n    border-collapse: collapse;\n\n    font-size: 10pt;\n\n    table-layout: fixed;\n\n  }\n \n  .quotation-table th,\n\n  .quotation-table td {\n\n    border: 1.5px solid #3c3835;\n\n    padding: 6px;\n\n    vertical-align: top;\n\n    word-break: break-word;\n\n  }\n \n  .quotation-table th {\n\n    text-align: center;\n\n    font-weight: bold;\n\n    background-color: #9e8976;\n\n    color: white;\n\n  }\n \n  .quotation-table td.item-group-cell {\n\n    background-color: #9e8976;\n\n    font-weight: bold;\n\n    color: white;\n\n  }\n \n  .quotation-table th:nth-child(1),\n\n  .quotation-table td:nth-child(1) { width: 14%; }\n \n  .quotation-table th:nth-child(2),\n\n  .quotation-table td:nth-child(2) { width: 20%; }\n \n  .quotation-table th:nth-child(3),\n\n  .quotation-table td:nth-child(3) { width: 10%; text-align: center; }\n \n  .quotation-table th:nth-child(4),\n\n  .quotation-table td:nth-child(4) { width: 45%; }\n \n  .quotation-table th:nth-child(5),\n\n  .quotation-table td:nth-child(5) { width: 12%; text-align: center; }\n \n  .g-total-row td {\n\n    font-weight: bold;\n\n    text-align: center;\n\n    padding: 10px;\n\n    border: 1.5px solid #3c3835;\n\n  }\n \n  .spec-table, .terms-table {\n\n    width: 100%;\n\n    border-collapse: collapse;\n\n    font-size: 10pt;\n\n    margin-top: 12mm;\n\n  }\n \n  .spec-table td, .terms-table td {\n\n    border: 1.5px solid #3c3835;\n\n    padding: 6px;\n\n    vertical-align: top;\n\n    line-height: 1.4;\n\n  }\n \n  .spec-table th, .terms-table th {\n\n    border: 1.5px solid #3c3835;\n\n    text-align: center;\n\n    font-weight: bold;\n\n    background-color: #9e8976;\n\n    color: white;\n\n    padding: 8px;\n\n    font-size: 12pt;\n\n  }\n \n  @media print {\n\n    body {\n\n      -webkit-print-color-adjust: exact;\n\n      print-color-adjust: exact;\n\n    }\n \n    .quotation-table thead {\n\n       display: table-row-group !important;\n\n    }\n\n  }\n</style>\n \n<!-- PAGE 1: Cover Image -->\n<div class=\"page\">\n<img src=\"/files/Premium.png\" style=\"width: 100%; height: auto;\" />\n</div>\n \n<!-- PAGE 2: Info Tables + Quotation -->\n<div style=\"page-break-before: always;\"></div>\n<div class=\"page\">\n<div class=\"info-table-wrapper\">\n<table>\n<tr><td>Project</td><td>{{ doc.name }}</td></tr>\n<tr><td>Client Name (first Party)</td><td>{{ doc.customer_name or \"\" }}</td></tr>\n<tr><td>Location</td>\n<td>\n\n          {% if doc.customer_address %}\n\n            {{ frappe.get_doc(\"Address\", doc.customer_address).city or \"\" }}\n\n          {% else %}-{% endif %}\n</td>\n</tr>\n<tr><td>Contractor (Second Party)</td><td>Mr. ANIT JOHN</td></tr>\n<tr><td>Design</td><td>GREENBAY INTERIORZ</td></tr>\n</table>\n<table>\n<tr><td>Estimate Date</td><td>{{ frappe.utils.formatdate(doc.transaction_date, \"dd-mm-yyyy\") }}</td></tr>\n<tr><td>Estimate Valid Till</td><td>{{ frappe.utils.formatdate(doc.valid_till, \"dd-mm-yyyy\") }}</td></tr>\n<tr><td>Client Manager</td><td>{{ doc.contact_person or \"Mr. ABHI\" }}</td></tr>\n<tr><td>Contact</td><td>8593003009</td></tr>\n<tr><td>Email</td><td><a href=\"mailto:info@greenbayinteriorz.com\">info@greenbayinteriorz.com</a></td></tr>\n</table>\n</div>\n \n  <div style=\"background-color: #9e8976; color: white; font-weight: bold; text-align: center; padding: 6px; margin-top: 12mm; margin-bottom: 5mm; font-size: 11pt;\">\n\n    SUB: ESTIMATE FOR WOOD WORKS & ACCESSORIES\n</div>\n \n  <table class=\"quotation-table\">\n<thead>\n<tr><th>Item Group</th><th>Item Name</th><th>Qty</th><th>Description</th><th>Rate</th></tr>\n</thead>\n<tbody>\n\n      {% set group_counts = {} %}\n\n      {% set item_counts = {} %}\n\n      {% for row in doc.items %}\n\n        {% set group_counts = group_counts.update({row.item_group: group_counts.get(row.item_group, 0) + 1}) if group_counts.get(row.item_group) else group_counts.update({row.item_group: 1}) %}\n\n        {% set item_counts = item_counts.update({row.item_name: item_counts.get(row.item_name, 0) + 1}) if item_counts.get(row.item_name) else item_counts.update({row.item_name: 1}) %}\n\n      {% endfor %}\n\n      {% set rendered_groups = [] %}\n\n      {% set rendered_items = [] %}\n\n      {% for row in doc.items %}\n<tr>\n\n          {% if row.item_group not in rendered_groups %}\n<td class=\"item-group-cell\" rowspan=\"{{ group_counts[row.item_group] }}\">{{ row.item_group }}</td>\n\n            {% set _ = rendered_groups.append(row.item_group) %}\n\n          {% endif %}\n\n          {% if row.item_name not in rendered_items %}\n<td rowspan=\"{{ item_counts[row.item_name] }}\">{{ row.item_name }}</td>\n\n            {% set _ = rendered_items.append(row.item_name) %}\n\n          {% endif %}\n<td>{{ row.qty }} {{ row.uom }}</td>\n<td>{{ row.description or \"\" }}</td>\n<td>{{ \"{:,.0f}\".format(row.rate) }}</td>\n</tr>\n\n      {% endfor %}\n<tr class=\"g-total-row\">\n<td colspan=\"5\">G. TOTAL : â‚¹{{ \"{:,.0f}\".format(doc.total) }}</td>\n</tr>\n</tbody>\n</table>\n</div>\n \n<!-- PAGE 3: Specifications and Terms -->\n<div style=\"page-break-before: always;\"></div>\n<div class=\"page\" style=\"padding-top: 20mm !important;\">\n<table class=\"spec-table\">\n<thead>\n<tr><th colspan=\"2\">SPECIFICATIONS</th></tr>\n</thead>\n<tbody>\n\n      {% for row in doc.custom_specifications %}\n<tr><td style=\"font-weight: bold; width: 30%;\">{{ row.material }}</td><td>{{ row.specification }}</td></tr>\n\n      {% endfor %}\n</tbody>\n</table>\n \n  <table class=\"terms-table\">\n<thead>\n<tr><th>TERMS & CONDITIONS</th></tr>\n</thead>\n<tbody>\n\n      {% set clean_terms = (doc.terms or \"\").replace(\"<br>\", \" \").replace(\"<br/>\", \" \").replace(\"<br />\", \" \").replace(\"</p>\", \" \").replace(\"<p>\", \"\") %}\n\n      {% for sentence in frappe.utils.strip_html(clean_terms).split('.') %}\n\n        {% if sentence.strip() %}\n<tr><td>{{ loop.index }}. {{ sentence.strip() }}.</td></tr>\n\n        {% endif %}\n\n      {% endfor %}\n</tbody>\n</table>\n \n  <div style=\"margin-top: 30mm; font-size: 10pt; font-weight: bold;\">\n<table style=\"width: 100%; border: none;\">\n<tr>\n<td style=\"width: 50%; vertical-align: top;\">\n<div><strong>{{ frappe.utils.formatdate(doc.transaction_date, \"dd-mm-yyyy\") }}</strong></div>\n<div><strong>Kozhikode</strong></div>\n</td>\n<td style=\"width: 50%; text-align: right; vertical-align: bottom;\">\n<div style=\"height: 40px;\"></div>\n<div><strong>GREENBAY INTERIORZ</strong></div>\n</td>\n</tr>\n</table>\n</div>\n</div>\n\n ",
