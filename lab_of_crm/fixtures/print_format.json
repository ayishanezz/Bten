  "html": "<style>\n  @page {\n    margin: 0;\n    size: A4;\n  }\n\n  body, html {\n    margin: 0;\n    padding: 0;\n    font-family: \"Helvetica\", sans-serif;\n  }\n\n  .print-format {\n    width: 100%;\n    padding: 0;\n    margin: 0;\n  }\n\n  .header-image {\n    width: 100%;\n    height: auto;\n    margin-bottom: 30px;\n  }\n\n  .info-row {\n    display: flex;\n    justify-content: space-between;\n    padding: 0 60px;\n    font-size: 13px;\n    margin-top: 10px;\n  }\n\n  .left-info {\n    flex: 1;\n    line-height: 1.5;\n    white-space: pre-line;\n  }\n\n  .right-info {\n    flex: 1;\n    display: flex;\n    justify-content: flex-end;\n  }\n\n  .right-info table {\n    font-size: 13px;\n    line-height: 1.5;\n    text-align: right;\n  }\n\n  .right-info td {\n    padding: 2px 8px;\n    white-space: nowrap;\n  }\n\n  .highlight {\n    color: #d62857;\n    font-weight: bold;\n  }\n\n  .quotation-table-wrapper {\n    padding: 0 60px;\n  }\n\n  .quotation-table {\n    width: 100%;\n    border-collapse: collapse;\n    margin: 40px 0 0 0;\n    font-size: 13px;\n  }\n\n  .quotation-table thead {\n      background-color: #b82663;\n  }\n\n  .quotation-table thead th {\n      color: #fff !important;\n  }\n\n  .quotation-table th,\n  .quotation-table td {\n    padding: 10px;\n    text-align: left;\n  }\n\n  .quotation-table td {\n    background-color: #efefef;\n  }\n\n  .summary-section {\n    width: 100%;\n    padding: 20px 60px 0 60px;\n    font-size: 13px;\n  }\n\n  .summary-row {\n    display: flex;\n    justify-content: space-between;\n    padding: 2px 0;\n  }\n\n  .summary-label,\n  .summary-value {\n    color: #000;\n    white-space: nowrap;\n  }\n\n  .additional-table-wrapper {\n    width: 100%;\n    padding: 20px 60px;\n    font-size: 13px;\n  }\n\n  .additional-table {\n    width: 100%;\n    border-collapse: collapse;\n    border: 2px solid #b82663;\n    margin-top: 10px;\n  }\n\n  .additional-table thead tr th {\n    background-color: #b82663 !important;\n    color: #fff !important;\n  }\n\n  .additional-table th,\n  .additional-table td {\n    padding: 10px;\n    text-align: left;\n    background-color: #efefef;\n    border: 3px solid #ffffff;\n    vertical-align: top;\n  }\n\n  .additional-table img {\n    max-width: 60px;\n    max-height: 60px;\n    object-fit: contain;\n  }\n  .additional-table tbody td {\n    font-size: 12px; /* smaller text inside table body */\n  }\n  .quotation-table-wrapper,\n  .additional-table-wrapper,\n  .summary-section{\n      padding-left: 60px !important;\n      padding-right: 60px !important;\n      box-sizing: border-box;\n  }\n  .additional-table {\n      width: 100% !important;\n      border-collapse: collapse;\n      table-layout: auto; /* allow natural column widths */\n  }\n  .additional-table th:nth-child(1),\n  .additional-table td:nth-child(1) {\n      width: 5%;\n  }\n\n  .additional-table th:nth-child(3),\n  .additional-table td:nth-child(3),\n  .additional-table th:nth-child(4),\n  .additional-table td:nth-child(4),\n  .additional-table th:nth-child(5),\n  .additional-table td:nth-child(5) {\n      width: 8%;\n  }\n\n  .additional-table th:nth-child(7),\n  .additional-table td:nth-child(7) {\n      width: 12%; /* image column */\n  }\n\n\n  .grand-total{\n        background-color: #ddd;\n        padding: 10px 20px;\n        font-weight: bold;\n        font-size: 14px;\n        display: flex;\n        justify-content: space-between;\n        width: 100%;\n  }\n  thead {\n  display: table-header-group; /* default â€” repeats */\n  }\n\n  @media print {\n    thead {\n      display: table-row-group; /* disable repeating */\n    }\n  }\n  \n  /* Page break control for tables and sections */\n  .quotation-table-wrapper,\n  .summary-section,\n  .additional-table-wrapper,\n  .grand-total,\n  .additional-table-wrapper table {\n    page-break-inside: avoid;\n    page-break-before: auto;\n    page-break-after: auto;\n  }\n\n  table {\n    page-break-inside: auto;\n    border-spacing: 0;\n  }\n\n</style>\n\n<div class=\"print-format\">\n  <!-- Header -->\n  <img src=\"/files/JBM%20header1.png\" class=\"header-image\">\n\n  <!-- Info Row -->\n  <div class=\"info-row\">\n    <div class=\"left-info\">\n      <strong>To</strong><br>\n      {{ doc.customer_name }}<br>\n      {{ doc.customer_address or '' }}\n    </div>\n    <div class=\"right-info\">\n      <table>\n        <tr>\n          <td><strong>Date</strong></td>\n          <td>{{ frappe.utils.format_date(doc.transaction_date) }}</td>\n        </tr>\n        <tr>\n          <td><strong class=\"highlight\">Finish</strong></td>\n          <td class=\"highlight\">Wpc & Laminate</td>\n        </tr>\n        <tr>\n          <td><strong>Customer</strong></td>\n          <td>{{ doc.customer_name }}</td>\n        </tr>\n        <tr>\n          <td><strong>Delivery</strong></td>\n          <td>{{ frappe.utils.format_date(doc.valid_till) }}</td>\n        </tr>\n      </table>\n    </div>\n  </div>\n\n  <!-- Quotation Table -->\n  <div class=\"quotation-table-wrapper\">\n    <table class=\"quotation-table\">\n      <thead>\n        <tr>\n          <th style=\"width: 50%;\">ITEMS</th>\n          <th style=\"width: 10%;\">QTY</th>\n          <th style=\"width: 20%;\">PRICE</th>\n          <th style=\"width: 20%;\">VALUE</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for item in doc.items %}\n        <tr>\n          <td><strong>{{ item.custom_items_group or '' }}</strong> - {{ item.item_code or '' }} : {{ item.description }}</td>\n          <td>{{ item.qty }}</td>\n          <td>{{ \"{:,.0f}\".format(item.rate or 0) }}</td>\n          <td>{{ \"{:,.0f}\".format(item.amount or 0) }}</td>\n        </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  </div>\n\n  <!-- First Summary Section -->\n  <div class=\"summary-section\">\n    <div class=\"summary-row\">\n      <div class=\"summary-label\">Total</div>\n      <div class=\"summary-value\">\n        {{ \"{:,.2f}\".format(doc.total or 0) }}\n      </div>\n    </div>\n\n    <div class=\"summary-row\">\n      <div class=\"summary-label\">Discount</div>\n      <div class=\"summary-value\">\n        {% set discount = (doc.total or 0) * (doc.additional_discount_percentage or 0) / 100 %}\n        {{ \"{:,.2f}\".format(discount) }}\n      </div>\n    </div>\n\n    <div class=\"summary-row\">\n      <div class=\"summary-label\">After Discount</div>\n      <div class=\"summary-value\">\n        {{ \"{:,.2f}\".format((doc.total or 0) - discount) }}\n      </div>\n    </div>\n  </div>\n\n  <!-- Additional Items Table -->\n  {% if doc.custom_additional_items %}\n  <div class=\"additional-table-wrapper\">\n    <table class=\"additional-table\">\n      <thead>\n        <tr>\n          <th>SI. No</th>\n          <th>Accessories - Work Area</th>\n          <th>Qty</th>\n          <th>Price</th>\n          <th>Value</th>\n          <th>Brand</th>\n          <th>Image</th>\n        </tr>\n      </thead>\n      <tbody>\n        {% for i, item in enumerate(doc.custom_additional_items) %}\n        <tr>\n          <td>{{ i + 1 }}</td>\n          <td>{{ item.item_code or '' }}</td>\n          <td>{{ item.qty or '' }}</td>\n          <td>{{ \"{:,.0f}\".format(item.rate or 0) }}</td>\n          <td>{{ \"{:,.0f}\".format(item.amount or 0) }}</td>\n          <td>{{ item.custom_items_group or '' }}</td>\n          <td>\n            {% if item.image %}\n              <img src=\"{{ item.image }}\">\n            {% endif %}\n          </td>\n        </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  </div>\n\n  <!-- Second Summary Section -->\n  <div class=\"summary-section\">\n    <div class=\"summary-row\">\n      <div class=\"summary-label\">Total</div>\n      <div class=\"summary-value\">{{ \"{:,.2f}\".format(doc.custom_additional_total or 0) }}</div>\n    </div>\n    <div class=\"summary-row\">\n      <div class=\"summary-label\">Discount</div>\n      <div class=\"summary-value\">\n          {% set discount = (doc.custom_additional_total or 0) * (doc.additional_discount_percentage or 0) / 100 %}\n          {{ \"{:,.2f}\".format(discount) }}\n      </div>\n    </div>\n    <div class=\"summary-row\">\n      <div class=\"summary-label\">After Discount</div>\n      <div class=\"summary-value\">\n        {{ \"{:,.2f}\".format((doc.custom_additional_total or 0) - discount) }}\n      </div>\n    </div>\n  </div>\n  {% endif %}\n  \n    <!-- Grand Total Section -->\n    <div style=\"padding: 0 60px; margin-top: 10px;\">\n      <div class=\"grand-total\">\n        <div>Grand Total</div>\n        <div>\n          {{\n            \"{:,.2f}\".format(\n              (doc.grand_total or 0) \n            )\n          }}\n        </div>\n      </div>\n    </div>\n    \n    <!-- Payment Terms Section (Safe & Fixed) -->\n    <div class=\"additional-table-wrapper\" style=\"padding-top: 20px;\">\n      <table style=\"width: 100%; border-collapse: collapse; font-size: 13px; border: 3px solid #ccc;\">\n        <thead>\n          <tr>\n            <th colspan=\"2\" style=\"text-align: center; font-size: 14px; border: 1px solid #ccc; background-color: #b82663; color: #fff;\">\n              <strong>Total Project Cost</strong>\n            </th>\n          </tr>\n          <tr>\n            <th style=\"width: 80%; border: 2px solid #ccc;\">Payment Terms</th>\n            <th style=\"width: 20%; text-align: right; border: 2px solid #ccc;\">Amount</th>\n          </tr>\n        </thead>\n        <tbody>\n          {% for row in doc.payment_schedule %}\n            <tr>\n              <td style=\"border: 2px solid #ccc;\">{{ row.description or '' }}</td>\n              <td style=\"text-align: right; border: 2px solid #ccc;\">\n                {{ \"{:,.2f}\".format(row.payment_amount or 0) }}\n              </td>\n            </tr>\n          {% endfor %}\n          <tr>\n            <td style=\"border: 2px solid #ccc; font-weight: bold;\">Total Project Cost</td>\n            <td style=\"text-align: right; border: 2px solid #ccc; font-weight: bold;\">\n              {{ \"{:,.2f}\".format(doc.grand_total or 0) }}\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n    \n    <!-- Terms & Conditions Section -->\n    {% if doc.terms %}\n      <div class=\"additional-table-wrapper\" style=\"padding-top: 10px; font-size: 13px;\">\n        <div>\n          {{ doc.terms }}\n        </div>\n      </div>\n    {% endif %}\n\n    <!-- Specification Table -->\n    {% if doc.custom_specifications %}\n      <div class=\"additional-table-wrapper\">\n        <table style=\"width: 100%; border-collapse: collapse; border: 3px solid #ccc; font-size: 13px;\">\n          <thead>\n            <tr>\n              <th colspan=\"2\" style=\"background-color: #b82663; color: white; text-align: center; padding: 10px; font-size: 14px;\">\n                Plywood and Other Specification\n              </th>\n            </tr>\n          </thead>\n          <tbody>\n            {% for row in doc.custom_specifications %}\n            <tr>\n              <td style=\"border: 3px solid #999; padding: 10px; width: 95%; vertical-align: top;\">\n                <strong>{{ row.material or '' }}</strong> - {{ row.specification or '' }}\n              </td>\n            </tr>\n            {% endfor %}\n          </tbody>\n        </table>\n      </div>\n    {% endif %}\n\n\n</div>",
